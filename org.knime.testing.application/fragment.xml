<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.disturber.DisturberNodeFactory"
            />
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.differModelContent.DiffModelContentFactory"
            />
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.failing.FailingNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.credentialsvalidate.CredentialsValidateNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.datagenerator.TestDataNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.file.DifferFileNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.imagecomp.ImageCompNodeFactory"
            deprecated="true"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.internal.nodes.differ.DifferenceCheckerNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.internal.nodes.image.ImageDifferNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.config.TestConfigNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.internal.nodes.pmml.PMMLDifferenceCheckerNodeFactory"
            >
      </node>
      <node
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.create.FileStoreCreateNodeFactory"
            >
      </node>
      <node
            after="org.knime.testing.node.filestore.create.FileStoreCreateNodeFactory"
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.create.port.FileStorePortCreateNodeFactory"
            >
      </node>
      <node
            after="org.knime.testing.node.filestore.create.port.FileStorePortCreateNodeFactory"
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.check.FileStoreTestNodeFactory"
            >
      </node>
      <node
            after="org.knime.testing.node.filestore.check.FileStoreTestNodeFactory"
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.createloopend.FileStoreCreateLoopEndNodeFactory"
            >
      </node>     
      <node
            after="org.knime.testing.node.filestore.createloopend.FileStoreCreateLoopEndNodeFactory"
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.fsloopend.LoopEndFileStorePortObjectTestNodeFactory"
            >
      </node>
      <node
            after="org.knime.testing.node.filestore.fsloopend.LoopEndFileStorePortObjectTestNodeFactory"
            category-path="/testing/filestore"
            factory-class="org.knime.testing.node.filestore.fsobject2cell.FileStoreObjectToCellNodeFactory"
            >
      </node>
      <node
            after="org.knime.testing.node.filestore.fsobject2cell.FileStoreObjectToCellNodeFactory"
            category-path="/testing/filestore"
            deprecated="false"
            factory-class="org.knime.testing.node.blob.create.CreateTestBlobNodeFactory">
      </node>
      <node
            after="org.knime.testing.node.blob.create.CreateTestBlobNodeFactory"
            category-path="/testing/filestore"
            deprecated="false"
            factory-class="org.knime.testing.node.blob.verify.VerifyTestBlobNodeFactory">
      </node>
      <node
            category-path="/testing"
            factory-class="org.knime.testing.node.logging.LoggerOptionNodeFactory"
            >
      </node>
      <node
            category-path="/testing"
            deprecated="false"
            factory-class="org.knime.testing.node.database.DBConnectionCloserNodeFactory">
      </node>
      <node
            category-path="/testing"
            deprecated="false"
            factory-class="org.knime.testing.node.workflowcontext.TestWorkflowContextNodeFactory">
      </node>
      <node
            deprecated="false"
            factory-class="org.knime.testing.node.wfcapture.workflow2table.Workflow2TableNodeFactory"
            hidden="true">
      </node>
      <node
            deprecated="false"
            factory-class="org.knime.testing.node.workflowsummary.WorkflowSummaryNodeFactory"
            hidden="true">
      </node>
      <node
            factory-class="org.knime.testing.node.description.NodeFactoryPropertiesExtractorNodeFactory"
            hidden="true">
      </node>
      <node
            category-path="/testing"
            deprecated="false"
            factory-class="org.knime.testing.node.io.filehandling.file.FileDifferenceCheckerNodeFactory"
            hidden="false">
      </node>
      <node
            deprecated="false"
            factory-class="org.knime.testing.node.workflowsummary.WorkflowSummaryConvertionTestNodeFactory"
            hidden="true">
      </node>
      <node
            deprecated="false"
            factory-class="org.knime.testing.node.extractor.NodeListExtractorNodeFactory"
            category-path="/testing"
            hidden="false">
      </node>
      <node
            deprecated="false"
            factory-class="org.knime.testing.node.metrics.MetricsReaderNodeFactory"
            category-path="/testing"
            hidden="false">
      </node>
      <node
            category-path="/testing"
            deprecated="false"
            factory-class="org.knime.testing.node.reporting2.ReportToTableNodeFactory"
            hidden="false">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/testing.png"
            level-id="testing"
            name="Testing"
            path="/"/>
      <category
            description="Contains nodes to test the handling of file stores in the framework"
            icon="icons/testing.png"
            level-id="filestore"
            locked="true"
            name="File Stores and Blobs"
            path="/testing"/>
   </extension>
   <extension
	     id="KNIME_TESTING_APPLICATION"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="org.knime.testing.core.KNIMETestingApplication"/>
      </application>
   </extension>
   <extension
         id="KNIME_PRINT_NODES_STDOUT_APPLICATION"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.knime.testing.core.KNIMEPrintNodesStdOutApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.knime.testing.DifferenceCheckerFactory">
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.EqualityChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.EpsilonNumberChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.StringChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.PngPHashChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.PngDHashChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.PngFullDifferenceChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.PngHistogramDifferenceChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.URIContentDifferenceChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.IgnoreChecker$Factory">
      </DifferenceCheckerFactory>
      <DifferenceCheckerFactory
            class="org.knime.testing.internal.diffcheckers.XMLChecker$Factory">
      </DifferenceCheckerFactory>
   </extension>
   <extension
         id="NGTestflowRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.core.ng.TestflowRunnerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="NGUnittestRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.core.ng.UnittestRunnerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="TestflowRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.headless.TestflowRunnerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="UnittestRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.headless.UnittestRunnerApplication">
         </run>
      </application>
   </extension>
            <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.knime.workbench.explorer.view"
            objectClass="org.knime.workbench.explorer.view.ContentObject">
         <action
               class="org.knime.testing.internal.ui.RunTestflowAction"
               enablesFor="+"
               icon="icons/execute_testflow.png"
               id="org.knime.testing.runWorkflowTest"
               label="Run as workflow test">
         </action>
      </objectContribution>
   </extension>
            <extension
                  id="HeadlessNodeInstantiation"
                  point="org.eclipse.core.runtime.applications">
               <application
                     cardinality="singleton-global"
                     thread="main"
                     visible="true">
                  <run
                        class="org.knime.testing.core.ng.HeadlessNodeInstantiationApplication">
                  </run>
               </application>
            </extension>
</fragment>
