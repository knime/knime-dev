<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         id="TestflowRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.headless.TestflowRunnerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="UnittestRunner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.knime.testing.headless.UnittestRunnerApplication">
         </run>
      </application>
   </extension>   
</fragment>
