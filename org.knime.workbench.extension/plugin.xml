<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.knime.workbench.ui.wizards.category"
            class="org.knime.workbench.extension.wizards.NewKNIMEPluginWizard"
            descriptionImage="icons/pluginWizard32x32.png"
            finalPerspective="org.eclipse.pde.ui.PDEPerspective"
            hasPages="true"
            icon="icons/pluginWizard16x16.png"
            id="org.knime.workbench.extension.wizards.NewKNIMEPluginWizard"
            name="Create a new KNIME Node-Extension"
            preferredPerspectives="org.eclipse.pde.ui.PDEPerspective"
            project="true">
         <description>
            This wizards creates a new plugin project with a initial project layout suitable for writing new nodes that can be used in the KNIME Workbench.
         </description>
      </wizard>
   </extension>

</plugin>
